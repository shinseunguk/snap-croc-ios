// Release Configuration

// Build Settings
SWIFT_ACTIVE_COMPILATION_CONDITIONS = $(inherited) RELEASE
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) RELEASE=1

// App Configuration
APP_NAME = SnapCroc
BUNDLE_ID_SUFFIX =
PRODUCT_BUNDLE_IDENTIFIER = com.snapcroc$(BUNDLE_ID_SUFFIX)

// API Configuration
API_BASE_URL = https:/$()/api.snapcroc.com
ENABLE_DEBUG_LOGGING = NO

// Optimization
SWIFT_OPTIMIZATION_LEVEL = -O
SWIFT_COMPILATION_MODE = wholemodule
